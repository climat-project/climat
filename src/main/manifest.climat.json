{
  "name": "climat",
  "action": "",
  "children": [
    {
      "name": "exec",
      "parameters": ["req:arg:pathToJson:p", "req:arg:command:c"],
      "action": "climat.exec('$(pathToJson)', '$(command)')"
    },
    {
      "name": "execNoValidation",
      "parameters": ["req:arg:pathToJson:p", "req:arg:command:c"],
      "action": "climat.execNoValidation('$(pathToJson)', '$(command)')"
    },
    {
      "name": "validate",
      "parameters": ["req:arg:pathToJson:p"],
      "action": "climat.validate('$(pathToJson)')"
    },
    {
      "name": "uninstall",
      "parameters": ["req:arg:name:p"],
      "action": "climat.uninstall('$(name)')"
    },
    {
      "name": "install",
      "parameters": ["req:arg:pathToJson:p"],
      "action": "climat.install('$(pathToJson)')"
    }
  ]
}
